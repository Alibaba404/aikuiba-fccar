<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.aikuiba.mapper.CustomerSummaryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.aikuiba.pojo.domain.CustomerSummary">
        <id column="id" property="id" />
        <result column="total_orders" property="totalOrders" />
        <result column="total_fine" property="totalFine" />
        <result column="voucher_numbers" property="voucherNumbers" />
        <result column="cancel_orders" property="cancelOrders" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, total_orders, total_fine, voucher_numbers, cancel_orders
    </sql>

</mapper>
